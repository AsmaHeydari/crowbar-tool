module CallS;
data Spec = ObjInv(Bool) | Ensures(Bool) | Requires(Bool) | WhileInv(Bool);

interface I { 
	[Spec : Requires(mi > mj)] 
	[Spec : Requires(mj > 0)] 
	[Spec : Ensures(result > mi)] 
	Int m(Int mi, Int mj);
}
interface K { 
	[Spec : Ensures(result > 0)] 
	Int o();
}
interface J extends I, K{
	[Spec : Requires(ni < 0)] 
	Int n(Int ni);
}

[Spec: ObjInv(this.f >= 0)]
class C() implements I{
	Int f = 0;	
	
	[Spec : Ensures(result > this.f)] 
	Int m(Int mi, Int mj){
		return mi+mj+this.f;
	}
}

[Spec: Requires(this.other != null)]
[Spec: ObjInv(this.other != null && this.g > 0)]
class D(I other) implements J {
	Int g = 1;	

	[Spec : Ensures(this.g > 1)] 
	Int m(Int mi, Int mj){
		this.g = this.g + 1;
		return mi+mj;
	}
	
	Int o(){
		other!m(this.g+1,this.g);
		return 1;
	}
	
	[Spec : Ensures(result != ni)] 
	[Spec : Ensures(result != this.g)] 
	Int n(Int ni){
	    	return this.g-ni;
	}
	
}
          

{
  I i = new C();
  J k  = new D(i);
  Fut<Int> f = k!n(-10);
}
